<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.members.mapper.MembersMapper">

	
	<select id="getMember" parameterType="com.example.demo.members.dto.MemberDto" resultType="com.example.demo.members.dto.MemberDto">
		SELECT *
		FROM members
		WHERE member_id = #{memberId}
	</select>
	
</mapper>	 